<!-- Delegating build script, used by cruisecontrol to build MY_PROJECT_1.
     Note that the basedir is set to the checked out project -->
<project name="SPICS_JSF"
        default="svnup"
        basedir="checkout/spics.jsf">
   
   <target name="svnup">
      	<!-- Get the latest from svn -->
   	<exec executable="svn">
		<arg line="up"/>
	</exec>
   </target>
</project>
	
